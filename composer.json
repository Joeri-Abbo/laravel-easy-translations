{
  "name": "joeri-abbo/laravel-easy-translations",
  "description": "Laravel laravel-easy-translations support",
  "type": "library",
  "version": "dev-master",
  "authors": [
    {
      "name": "Joeri Abbo",
      "email": "joeriabbo@hotmail.com"
    }
  ],
  "require-dev": {
    "orchestra/testbench": "^7.3",
    "pestphp/pest": "^1.21",
    "phpunit/phpunit": "^9.5.10",
    "squizlabs/php_codesniffer": "^3.6.2",
    "vimeo/psalm": "^4.23"
  },
  "autoload": {
    "psr-4": {
      "JoeriAbbo\\LaravelEasyTranslations\\": "src/"
    },
    "files": [
      "helpers/general.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JoeriAbbo\\LaravelEasyTranslations\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true,
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JoeriAbbo\\LaravelEasyTranslations\\LaravelEasyTranslationsPackageServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ],
    "test": "vendor/bin/pest"
  }
}
